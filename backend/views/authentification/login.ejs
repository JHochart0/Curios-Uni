    <h1 id="login-h1">Se connecter</h1>
    <form id="login-form">
        <label for="email">Adresse e-mail</label>
        <input type="text" name="email" id="email" required>

        <label for="password">Mot de passe <span class="mandatory-asterisk">*</span></label>
